{
  "name": "npm-workspaces-demo",
  "version": "1.0.0",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "npm-workspaces-demo",
      "version": "1.0.0",
      "license": "MIT",
      "workspaces": [
        "./packages/*",
        "./long/nested/*"
      ]
    },
    "long/nested/package-c": {
      "version": "1.0.0",
      "dependencies": {
        "@test/package-a": "1.0.0"
      }
    },
    "node_modules/@test/package-a": {
      "resolved": "packages/package-a",
      "link": true
    },
    "node_modules/@types/node": {
      "version": "12.20.55",
      "resolved": "https://registry.npmjs.org/@types/node/-/node-12.20.55.tgz",
      "integrity": "sha512-J8xLz7q2OFulZ2cyGTLE1TbbZcjpno7FaN6zdJNrgAdrJ+DZzh/uFR6YrTb4C+nXakvud8Q4+rbhoIWlYQbUFQ==",
      "dev": true
    },
    "node_modules/package-b": {
      "resolved": "packages/package-b",
      "link": true
    },
    "node_modules/package-c": {
      "resolved": "long/nested/package-c",
      "link": true
    },
    "packages/package-a": {
      "name": "@test/package-a",
      "version": "1.0.0"
    },
    "packages/package-b": {
      "version": "1.0.0",
      "dependencies": {
        "@test/package-a": "1.0.0",
        "package-c": "1.0.0"
      },
      "devDependencies": {
        "@types/node": "^12.12.12"
      }
    }
  }
}
